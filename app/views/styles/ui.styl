#toolbar
	position absolute
	top 2px
	left 0
	right 0
	margin 0 auto
	width 335px
	height 100px
	#border 1px solid red
	overflow hidden

	a
		color red

	svg
		polygon:hover
			stroke #466927
			stroke-width 0.04

		text
			pointer-events none
			text-anchor middle
			-webkit-touch-callout none
			-webkit-user-select none
			-khtml-user-select none
			-moz-user-select none
			-ms-user-select none
			user-select none
